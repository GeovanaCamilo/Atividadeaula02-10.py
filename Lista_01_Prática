        "valor_reais = float(input(\"Digite o valor em reais: R$ \"))\n",
        "\n",
        "valor_dolares = valor_reais / taxa_conversao\n",
        "\n",
        "\n",
        "print(\"O valor em dólares é: $\", round(valor_dolares, 2), \"USD\")\n",
        "\n"
